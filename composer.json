{
    "name": "lvieira/rospdf-laravel",
    "description": "Rospdf PHP integration for Laravel",
    "type": "library",
    "require": {
        "illuminate/support": "^5.4",
        "rospdf/pdf-php": "^0.12.31",
        "illuminate/database": "^5.4",
        "symfony/http-foundation": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas S. Vieira",
            "email": "lucassouzavieiraengcomp@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "vendor/Illuminate/Foundation/helpers.php",
